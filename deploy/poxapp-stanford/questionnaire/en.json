  "variables": [
    {
      "expression": {
        ">": [
          {
            "var": "q_contact_date"
          },
          {
            "-": [
              {
                "var": "now"
              },
              {
                "*": [
                  21,
                  86400
                ]
              }
            ]
          }
        ]
      },
      "id": "v_contact_relevant"
    },
    {
      "expression": {
        "if": [
          {
            "and": [
              {
                "var": "q_contact.option.yes"
              },
              {
                "!": {
                  "var": "v_contact_relevant"
                }
              }
            ]
          },
          {
            "-": [
              {
                "var": "score.Exposition"
              },
              3
            ]
          },
          {
            "var": "score.Exposition"
          }
        ]
      },
      "id": "v_exposition"
    },
    {
      "expression": {
        "if": [
          {
            "var": "v_contact_relevant"
          },
          {
            ">": [
              {
                "var": "q_symptoms_date"
              },
              {
                "var": "q_contact_date"
              }
            ]
          },
          false
        ]
      },
      "id": "v_symptoms_after_contact"
    },
    {
      "expression": {
        "if": [
          {
            "and": [
              {
                "var": "q_symptoms.option.fever"
              },
              {
                "<": [
                  {
                    "var": "q_temperature"
                  },
                  100
                ]
              }
            ]
          },
          {
            "-": [
              {
                "var": "score.Symptoms"
              },
              1
            ]
          },
          {
            "var": "score.Symptoms"
          }
        ]
      },
      "id": "v_symptoms2"
    },
    {
      "expression": {
        "if": [
          {
            "and": [
              {
                ">=": [
                  {
                    "var": "v_symptoms2"
                  },
                  8
                ]
              },
              {
                "==": [
                  {
                    "var": "v_exposition"
                  },
                  3
                ]
              },
              {
                "!": {
                  "var": "v_symptoms_after_contact"
                }
              }
            ]
          },
          {
            "-": [
              {
                "var": "v_symptoms2"
              },
              8
            ]
          },
          {
            "var": "v_symptoms2"
          }
        ]
      },
      "id": "v_symptoms"
    },
    {
      "expression": "<img src=\"https://poxapp.charite.de/assets/images/recommendation-green.svg\" alt=\"green\" width=\"20\" height=\"20\">",
